  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "<rootDir>",
      "src"
    ],
    "testRegex": ".*\\.e2e-spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^@app/(.*)$": "<rootDir>/src/app/$1",
      "@domain/(.*)$": "<rootDir>/src/domain/$1",
      "@infrastructure/(.*)$": "<rootDir>/src/infrastructure/$1"
    }
  }
